{
    "extends": "../tsconfig.base.json",
    "compilerOptions": {
        "outDir": "../out-tsc/app",
        "baseUrl": "./",
        "types": ["node"],
        "typeRoots": [ "../node_modules/@types" ]
    },
    "files": [
        "main.browser.ts",
        "polyfills.ts"
    ],
    "exclude": [
        "test.ts",
        "**/*.spec.ts",
        "node_modules"
    ],
    "include": [
        "src/**/*.d.ts",
        "src/**/*.ts"
    ]
}
